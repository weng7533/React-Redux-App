{
    "version": 2,
    "name": "React-Redux-App",
    "builds": [
        {
            "src": "React-Redux-App/package.json",
            "use": "@now/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "React-Redux-App/$1"
        }
    ]
}